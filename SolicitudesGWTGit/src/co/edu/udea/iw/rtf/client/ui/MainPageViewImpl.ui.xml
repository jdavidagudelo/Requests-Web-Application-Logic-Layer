<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<ui:style>
		.panel {
		background-color: ivory;
		}

		.button {
		background:
		#B5C1C5;
		background: -moz-linear-gradient(top, #e4f1f6 0%, #EEF5F7
		100%);
		background: -webkit-linear-gradient(top, #e4f1f6 0%, #EEF5F7
		100%);
		border: 2px solid #CED5D7;
		color: #000000;
		padding: 8px;
		width:
		100px;
		font-size: 18px;
		text-shadow: 0 0 10px #00EE00;
		display: block;
		border-radius: 4px;
		font-family: helvetica;
		margin: 30px auto;
		text-decoration: none;
		}

		.button:hover {
		background: #e4f1f6;
		background:
		-moz-linear-gradient(top, #e4f1f6 0%, #cfe6ef 100%);
		background:
		-webkit-linear-gradient(top, #e4f1f6 0%, #cfe6ef 100%);
		border: 2px
		solid #00EE00;
		color: #000000;
		padding: 10px;
		width: 100px;
		font-size:
		18px;
		text-shadow: 0 0 10px #00EE00;
		display: block;
		border-radius: 4px;
		}

		.textBox {
		border: 1px solid #CED5D7;
		border-radius: 1px;
		padding: 10px 80px;
		background-color: white;
		box-shadow: 0px 5px 10px #B5C1C5, 0 0 0 5px #EEF5F7 inset;
		text-align: center;
		margin-bottom: 15px;
		text-decoration: none;
		color: #000000;
		}

		.label {
		display: block;
		font-weight: bold;
		color: black;
		font-family: helvetica;
		text-align: center;
		text-decoration: none;
		}

	</ui:style>
	<g:HTMLPanel>
  <center>
    
	<g:VerticalPanel styleName="{style.panel}">

		<g:Button styleName="{style.button}" ui:field="buttonLogin"
			text="Asignar" />
		<g:Button styleName="{style.button}" ui:field="buttonRealizarSolicitud"
			text="Solicitar" />

	</g:VerticalPanel>
</center></g:HTMLPanel>
</ui:UiBinder>
